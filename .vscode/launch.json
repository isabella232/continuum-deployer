{
    // Verwendet IntelliSense zum Ermitteln möglicher Attribute.
    // Zeigen Sie auf vorhandene Attribute, um die zugehörigen Beschreibungen anzuzeigen.
    // Weitere Informationen finden Sie unter https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run current file",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Run CLI print-resources",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/continuum_deployer/app.py",
            "args": [
                "print-resources",
                "--file",
                "./charts/redis/redis.yaml"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Run CLI parse-resources",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/continuum_deployer/app.py",
            "args": [
                "parse-resources",
                "--file",
                "./resources/default.yaml"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Run Tests",
            "type": "python",
            "request": "launch",
            "module": "pytest"
        },
        {
            "name": "Run CLI match SAT",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/continuum_deployer/app.py",
            "args": [
                "nonint-match",
                "-r",
                "./resources/default.yaml",
                "-d",
                "./charts/sample_multi.yaml",
                "-s",
                "sat"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Run CLI match SAT with file output",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/continuum_deployer/app.py",
            "args": [
                "nonint-match",
                "-r",
                "./resources/default.yaml",
                "-d",
                "./charts/sample_multi.yaml",
                "-s",
                "sat",
                "-o",
                "./output_local/output.yaml"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Run CLI match Greedy",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/continuum_deployer/app.py",
            "args": [
                "nonint-match",
                "-r",
                "./resources/default.yaml",
                "-d",
                "./charts/sample_multi.yaml",
                "-s",
                "greedy"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Run CLI match GitLab",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/continuum_deployer/app.py",
            "args": [
                "nonint-match",
                "-r",
                "./resources/default.yaml",
                "-d",
                "./charts/gitlab/gitlab_edited.yaml",
                "-s",
                "greedy",
                "-o",
                "/tmp/output.yaml"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Run CLI interactively",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/continuum_deployer/app.py",
            "args": [
                "match",
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Run CLI interactively less-input yaml",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/continuum_deployer/app.py",
            "args": [
                "match",
                "-r",
                "resources/default.yaml",
                "-d",
                "charts/sample_multi.yaml"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Run CLI interactively less-input archive",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/continuum_deployer/app.py",
            "args": [
                "match",
                "-r",
                "resources/default.yaml",
                "-d",
                "charts/wordpress/wordpress.tgz"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Run Tests",
            "type": "python",
            "request": "launch",
            "module": "pytest"
        }
    ]
}